<connectorMetaData path="vm">
	<name>ASTM Reader</name>
	<author>NextGen Healthcare</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>3.8.1</mirthVersion>
	<url>http://www.nextgen.com</url>
	<description>This connector allows Mirth Connect to listen for incoming events from other channels in the same Mirth Connect instance.</description>
	<clientClassName>com.mirth.connect.connectors.astm.ASTMReader</clientClassName>
	<serverClassName>com.mirth.connect.connectors.astm.ASTMReceiver</serverClassName>
	<sharedClassName>com.mirth.connect.connectors.astm.ASTMReceiverProperties</sharedClassName>
	<library type="CLIENT" path="astm-client.jar" />
	<library type="SHARED" path="astm-shared.jar" />
	<library type="SERVER" path="astm-server.jar" />
	<transformers></transformers>
	<protocol>vm</protocol>
	<type>SOURCE</type>
</connectorMetaData>